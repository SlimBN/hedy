
@font-face {
  font-family: 'dekar';
  src: url('<%= asset_path('dekar.otf') %>');
}


@font-face {
  font-family: 'always';
  src: url('<%= asset_path('always.ttf') %>') format('truetype');
}


@font-face {
  font-family: 'alex';
  src: url('<%= asset_path('Alex.ttf') %>') format('truetype');
}


@font-face {
  font-family: 'photograph';
  src: url('<%= asset_path('photograph.ttf') %>') format('truetype');
}

@font-face {
  font-family: 'linowrite';
  src: url('<%= asset_path('linowrite.ttf') %>') format('truetype');
}



@font-face {
    font-family: 'BebasNeueRegular';
    src: url('<%= asset_path('BebasNeue-webfont.eot') %>');
    src: url('<%= asset_path('BebasNeue-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('BebasNeue-webfont.woff') %>') format('woff'),
         url('<%= asset_path('BebasNeue-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('BebasNeue-webfont.svg#BebasNeueRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}
